{
    "version": "1.5.0-alpha",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "./**/*.ts",
        "!./**/*_spec.ts",
        "!./node_modules/**/*.ts"
    ],
    "files": [
        "./src/ts/wurscht3000.ts",
        "./typings/jasmine/jasmine.d.ts",
        "./typings/tsd.d.ts"
    ],
    "compileOnSave": false
}
